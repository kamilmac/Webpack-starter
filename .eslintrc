{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "__DEV__": true
    },
    "rules": {
        "indent": [2, 4, {"SwitchCase": 1}],
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "import/no-named-as-default-member": 0,
        "import/prefer-default-export": 0,
        "no-use-before-define": 0,
        "no-else-return": 0,
        "no-console": 0,
        "max-len": [1, 100],
        "jsx-no-bind": 0
    }
}